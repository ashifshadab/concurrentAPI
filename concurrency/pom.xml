<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.ashifshadab</groupId>
	<artifactId>concurrency</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>concurrency</name>
	<description>Only Thread related concepts</description>
	<packaging>pom</packaging>
	<properties>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
	</properties>
	<modules>
		<module>Semaphore</module>
		<module>Notes</module>
		<module>Mutex</module>
		<module>CountDownLatch</module>
		<module>CyclicBarrier</module>
		<module>Exchanger</module>
		<module>Phaser</module>
		<module>Executor</module>
		<module>Lock</module>
		<module>atomic</module>
		<module>threadLocalRandom</module>
		<module>executorService</module>
		<module>ThreadPool</module>
		<module>FuturesAndCallables</module>
		<module>ForkJoin</module>
	</modules>
</project>